@prefix doap: <http://usefulinc.com/ns/doap#> .
@prefix fomp: <http://drobilla.net/plugins/fomp/> .
@prefix lv2: <http://lv2plug.in/ns/lv2core#> .
@prefix mod: <http://moddevices.com/ns/mod#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix units: <http://lv2plug.in/ns/extensions/units#> .

fomp:cs_chorus2
	a lv2:Plugin ,
		lv2:ChorusPlugin ;
	doap:name "CS Chorus 2" ;
	mod:brand "FOMP" ;
	mod:label "CS Chorus 2" ;
	doap:license <http://opensource.org/licenses/gpl-2.0> ;
	rdfs:comment """
Fomp is an LV2 port of the MCP, VCO, FIL, and WAH plugins by Fons Adriaensen.

Functionally identical to variant 1, but upsamples the input to the delay lines in an attempt to mitigate the errors produced by the linear interpolation at the output.

Features:
Modeled by FOMP
""" ;
	lv2:microVersion 0 ;
	lv2:minorVersion 0 ;
	lv2:optionalFeature lv2:hardRTCapable ;
	lv2:project fomp: ;
	lv2:port [
		a lv2:AudioPort ,
			lv2:InputPort ;
		lv2:index 0 ;
		lv2:name "Input" ;
		lv2:symbol "in"
	] , [
		a lv2:AudioPort ,
			lv2:OutputPort ;
		lv2:index 1 ;
		lv2:name "Output" ;
		lv2:symbol "out1"
	] , [
		a lv2:ControlPort ,
			lv2:InputPort ;
		lv2:default 0.0 ;
		lv2:index 2 ;
		lv2:maximum 30.0 ;
		lv2:minimum 0.0 ;
		mod:default 7.5 ;
		lv2:name "Delay" ;
		lv2:symbol "delay" ;
		units:unit units:ms
	] , [
		a lv2:ControlPort ,
			lv2:InputPort ;
		lv2:default 0.01 ;
		lv2:index 3 ;
		lv2:maximum 10.0 ;
		lv2:minimum 0.003 ;
		mod:default 0.2 ;
		lv2:name "Mod Frequency 1" ;
		lv2:shortName "Rate 1" ;
        lv2:portProperty  mod:tapTempo , mod:tempoRelatedDynamicScalePoints;
		lv2:portProperty <http://lv2plug.in/ns/ext/port-props#logarithmic> ;
		lv2:symbol "mod_freq_1" ;
		units:unit units:hz
	] , [
		a lv2:ControlPort ,
			lv2:InputPort ;
		lv2:default 0.0 ;
		lv2:index 4 ;
		lv2:maximum 10.0 ;
		lv2:minimum 0.0 ;
		mod:default 1.25 ;
		lv2:name "Mod Amplitude 1" ;
		lv2:shortName "Depth 1" ;
		lv2:symbol "mod_amp_1" ;
		units:unit units:ms
	] , [
		a lv2:ControlPort ,
			lv2:InputPort ;
		lv2:default 0.01;
		lv2:index 5 ;
		lv2:maximum 30.0 ;
		lv2:minimum 0.01 ;
		mod:default 0.55 ;
		lv2:name "Mod Frequency 2" ;
		lv2:shortName "Rate 2" ;
        lv2:portProperty  mod:tapTempo , mod:tempoRelatedDynamicScalePoints;
		lv2:portProperty <http://lv2plug.in/ns/ext/port-props#logarithmic> ;
		lv2:symbol "mod_freq_2" ;
		units:unit units:hz
	] , [
		a lv2:ControlPort ,
			lv2:InputPort ;
		lv2:default 0.0 ;
		lv2:index 6 ;
		lv2:maximum 3.0 ;
		lv2:minimum 0.0 ;
		mod:default 0.75 ;
		lv2:name "Mod Amplitude 2" ;
		lv2:shortName "Depth 2" ;
		lv2:symbol "mod_amp_2" ;
		units:unit units:ms
	] .
