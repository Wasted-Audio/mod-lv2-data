@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .

<http://kunz.corrupt.ch/products/tal-vocoder>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-tal-vocoder-ii.html> ;
        modgui:stylesheet <modgui/stylesheet-tal-vocoder-ii.css> ;
        modgui:screenshot <modgui/screenshot-tal-vocoder-ii.png> ;
        modgui:thumbnail <modgui/thumbnail-tal-vocoder-ii.png> ;
        modgui:brand "TAL" ;
        modgui:label "Vocoder-II" ;
        modgui:model "boxy" ;
        modgui:panel "7-knobs" ;
        modgui:color "red" ;
        modgui:knob "black" ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "tal_volume" ;
            lv2:name "VOL" ;
        ] , [
            lv2:index 1 ;
            lv2:symbol "harmonics" ;
            lv2:name "HARMONICS" ;
        ] , [
            lv2:index 2 ;
            lv2:symbol "noisevolume" ;
            lv2:name "NOISE V" ;
        ] , [
            lv2:index 3 ;
            lv2:symbol "sawvolume" ;
            lv2:name "SAW V" ;
        ] , [
            lv2:index 4 ;
            lv2:symbol "pulsevolume" ;
            lv2:name "PULSE V" ;
        ] , [
            lv2:index 5 ;
            lv2:symbol "suboscvolume" ;
            lv2:name "SUB VOL" ;
        ] , [
            lv2:index 6 ;
            lv2:symbol "suboscoctave" ;
            lv2:name "SUB OCt" ;
        ] ;
    ] .
