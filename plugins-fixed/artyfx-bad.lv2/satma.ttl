@prefix doap: <http://usefulinc.com/ns/doap#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix lv2: <http://lv2plug.in/ns/lv2core#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix epp: <http://lv2plug.in/ns/ext/port-props#> .
@prefix ui: <http://lv2plug.in/ns/extensions/ui#>.
@prefix pprop:  <http://lv2plug.in/ns/ext/port-props#>.
@prefix mod:    <http://moddevices.com/ns/mod#> .

<http://www.openavproductions.com/artyfx#satma/gui>
  a ui:X11UI;
  ui:binary <artyfx_ui.so>;
  lv2:optionalFeature ui:noUserResize ;
  lv2:extensionData   ui:idleInterface .
  

<http://www.openavproductions.com/artyfx#satma>
  a lv2:Plugin ,
    lv2:DistortionPlugin ;
  
  doap:name "Satma";
  doap:description "Distortion Maximizer";
  lv2:optionalFeature lv2:hardRTCapable ;
  rdfs:comment "A distortion maximizer effect.";
  lv2:minorVersion 2; lv2:microVersion 0;
  doap:license <http://usefulinc.com/doap/licenses/gpl> ;
  lv2:project <http://www.openavproductions.com/artyfx> ;
  mod:brand "OpenAV" ;

  ui:ui <http://www.openavproductions.com/artyfx#satma/gui>;
  
  lv2:optionalFeature <http://lv2plug.in/ns/lv2core#hardRTCapable> ,
                      <http://lv2plug.in/ns/ext/buf-size#boundedBlockLength> ;

  lv2:requiredFeature <http://lv2plug.in/ns/ext/urid#map> ;

  lv2:port [
      a lv2:InputPort, lv2:AudioPort ;
      lv2:index 0 ;
      lv2:symbol "input" ;
      lv2:name "Audio Input" ;
  ] , [
      a lv2:OutputPort, lv2:AudioPort ;
      lv2:index 1 ;
      lv2:symbol "output" ;
      lv2:name "Audio Output" ;
  ] , [
      a lv2:InputPort, lv2:ControlPort ;
      lv2:index 2 ;
      lv2:name "Distort" ;
      lv2:symbol "distort" ;
      lv2:default 0.000000 ;
      lv2:minimum 0.000000 ;
      lv2:maximum 1.000000 ;
  ] , [
      a lv2:InputPort, lv2:ControlPort ;
      lv2:index 3 ;
      lv2:name "Tone" ;
      lv2:symbol "tone" ;
      lv2:default 0.000000 ;
      lv2:minimum 0.000000 ;
      lv2:maximum 1.000000 ;
  ] , [
      a lv2:InputPort, lv2:ControlPort ;
      lv2:index 4 ;
      lv2:name "Active" ;
      lv2:symbol "active" ;
      lv2:default 1.000000 ;
      lv2:minimum 0.000000 ;
      lv2:maximum 1.000000 ;
      lv2:portProperty lv2:toggled ;
      lv2:portProperty pprop:notOnGUI ;
  ]
.
