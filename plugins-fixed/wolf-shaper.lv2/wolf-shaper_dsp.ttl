@prefix atom:  <http://lv2plug.in/ns/ext/atom#> .
@prefix doap:  <http://usefulinc.com/ns/doap#> .
@prefix foaf:  <http://xmlns.com/foaf/0.1/> .
@prefix lv2:   <http://lv2plug.in/ns/lv2core#> .
@prefix midi:  <http://lv2plug.in/ns/ext/midi#> .
@prefix mod:   <http://moddevices.com/ns/mod#> .
@prefix opts:  <http://lv2plug.in/ns/ext/options#> .
@prefix pg:    <http://lv2plug.in/ns/ext/port-groups#> .
@prefix patch: <http://lv2plug.in/ns/ext/patch#> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rsz:   <http://lv2plug.in/ns/ext/resize-port#> .
@prefix spdx:  <http://spdx.org/rdf/terms#> .
@prefix unit:  <http://lv2plug.in/ns/extensions/units#> .

<https://github.com/pdesaulniers/wolf-shaper>
    a lv2:WaveshaperPlugin, lv2:Plugin, doap:Project ;

    lv2:extensionData opts:interface ,
                      <http://lv2plug.in/ns/ext/state#interface> ,
                      <http://lv2plug.in/ns/ext/worker#interface> ;

    lv2:optionalFeature <http://lv2plug.in/ns/ext/buf-size#boundedBlockLength> ;

    lv2:requiredFeature opts:options ,
                        <http://lv2plug.in/ns/ext/urid#map> ,
                        <http://lv2plug.in/ns/ext/worker#schedule> ;

    opts:supportedOption <http://lv2plug.in/ns/ext/buf-size#nominalBlockLength> ,
                         <http://lv2plug.in/ns/ext/buf-size#maxBlockLength> ,
                         <http://lv2plug.in/ns/ext/parameters#sampleRate> ;

    lv2:port [
        a lv2:InputPort, lv2:AudioPort ;
        lv2:index 0 ;
        lv2:symbol "lv2_audio_in_1" ;
        lv2:name "Audio Input 1" ;
    ] ,
    [
        a lv2:InputPort, lv2:AudioPort ;
        lv2:index 1 ;
        lv2:symbol "lv2_audio_in_2" ;
        lv2:name "Audio Input 2" ;
    ] ;

    lv2:port [
        a lv2:OutputPort, lv2:AudioPort ;
        lv2:index 2 ;
        lv2:symbol "lv2_audio_out_1" ;
        lv2:name "Audio Output 1" ;
    ] ,
    [
        a lv2:OutputPort, lv2:AudioPort ;
        lv2:index 3 ;
        lv2:symbol "lv2_audio_out_2" ;
        lv2:name "Audio Output 2" ;
    ] ;

    lv2:port [
        a lv2:InputPort, atom:AtomPort ;
        lv2:index 4 ;
        lv2:name "Events Input" ;
        lv2:symbol "lv2_events_in" ;
        rsz:minimumSize 2048 ;
        atom:bufferType atom:Sequence ;
        atom:supports atom:String ;
    ] ;

    lv2:port [
        a lv2:OutputPort, atom:AtomPort ;
        lv2:index 5 ;
        lv2:name "Events Output" ;
        lv2:symbol "lv2_events_out" ;
        rsz:minimumSize 2048 ;
        atom:bufferType atom:Sequence ;
        atom:supports atom:String ;
    ] ;

    lv2:port [
        a lv2:InputPort, lv2:ControlPort ;
        lv2:index 6 ;
        lv2:name "Pre Gain" ;
        lv2:symbol "pregain" ;
        lv2:default 1 ;
        lv2:minimum 0 ;
        lv2:maximum 2 ;
    ] ,
    [
        a lv2:InputPort, lv2:ControlPort ;
        lv2:index 7 ;
        lv2:name "Wet" ;
        lv2:symbol "wet" ;
        lv2:default 1 ;
        lv2:minimum 0 ;
        lv2:maximum 1 ;
    ] ,
    [
        a lv2:InputPort, lv2:ControlPort ;
        lv2:index 8 ;
        lv2:name "Post Gain" ;
        lv2:symbol "postgain" ;
        lv2:default 1 ;
        lv2:minimum 0 ;
        lv2:maximum 1 ;
    ] ,
    [
        a lv2:InputPort, lv2:ControlPort ;
        lv2:index 9 ;
        lv2:name "Remove DC Offset" ;
        lv2:symbol "removedc" ;
        lv2:default 1 ;
        lv2:minimum 0 ;
        lv2:maximum 1 ;
        lv2:portProperty lv2:toggled ;
        lv2:portProperty lv2:integer ;
    ] ,
    [
        a lv2:InputPort, lv2:ControlPort ;
        lv2:index 10 ;
        lv2:name "Oversample" ;
        lv2:symbol "oversample" ;
        lv2:default 0 ;
        lv2:minimum 0 ;
        lv2:maximum 4 ;
        lv2:portProperty lv2:enumeration ;
        lv2:scalePoint [
            rdfs:label  "None" ;
            rdf:value 0 ;
        ] ,
        [
            rdfs:label  "2x" ;
            rdf:value 1 ;
        ] ,
        [
            rdfs:label  "4x" ;
            rdf:value 2 ;
        ] ,
        [
            rdfs:label  "8x" ;
            rdf:value 3 ;
        ] ,
        [
            rdfs:label  "16x" ;
            rdf:value 4 ;
        ] ;
        lv2:portProperty lv2:integer ;
    ] ,
    [
        a lv2:InputPort, lv2:ControlPort ;
        lv2:index 11 ;
        lv2:name "Bipolar Mode" ;
        lv2:symbol "bipolarmode" ;
        lv2:default 0 ;
        lv2:minimum 0 ;
        lv2:maximum 1 ;
        lv2:portProperty lv2:enumeration ;
        lv2:scalePoint [
            rdfs:label  "Unipolar" ;
            rdf:value 0 ;
        ] ,
        [
            rdfs:label  "Bipolar" ;
            rdf:value 1 ;
        ] ;
        lv2:portProperty lv2:toggled ;
        lv2:portProperty lv2:integer ;
    ] ,
    [
        a lv2:InputPort, lv2:ControlPort ;
        lv2:index 12 ;
        lv2:name "H Warp Type" ;
        lv2:symbol "warptype" ;
        lv2:default 0 ;
        lv2:minimum 0 ;
        lv2:maximum 6 ;
        lv2:portProperty lv2:enumeration ;
        lv2:scalePoint [
            rdfs:label  "None" ;
            rdf:value 0 ;
        ] ,
        [
            rdfs:label  "Bend +" ;
            rdf:value 1 ;
        ] ,
        [
            rdfs:label  "Bend -" ;
            rdf:value 2 ;
        ] ,
        [
            rdfs:label  "Bend +/-" ;
            rdf:value 3 ;
        ] ,
        [
            rdfs:label  "Skew +" ;
            rdf:value 4 ;
        ] ,
        [
            rdfs:label  "Skew -" ;
            rdf:value 5 ;
        ] ,
        [
            rdfs:label  "Skew +/-" ;
            rdf:value 6 ;
        ] ;
        lv2:portProperty lv2:integer ;
    ] ,
    [
        a lv2:InputPort, lv2:ControlPort ;
        lv2:index 13 ;
        lv2:name "H Warp Amount" ;
        lv2:symbol "warpamount" ;
        lv2:default 0 ;
        lv2:minimum 0 ;
        lv2:maximum 1 ;
    ] ,
    [
        a lv2:InputPort, lv2:ControlPort ;
        lv2:index 14 ;
        lv2:name "V Warp Type" ;
        lv2:symbol "vwarptype" ;
        lv2:default 0 ;
        lv2:minimum 0 ;
        lv2:maximum 6 ;
        lv2:portProperty lv2:enumeration ;
        lv2:scalePoint [
            rdfs:label  "None" ;
            rdf:value 0 ;
        ] ,
        [
            rdfs:label  "Bend +" ;
            rdf:value 1 ;
        ] ,
        [
            rdfs:label  "Bend -" ;
            rdf:value 2 ;
        ] ,
        [
            rdfs:label  "Bend +/-" ;
            rdf:value 3 ;
        ] ,
        [
            rdfs:label  "Skew +" ;
            rdf:value 4 ;
        ] ,
        [
            rdfs:label  "Skew -" ;
            rdf:value 5 ;
        ] ,
        [
            rdfs:label  "Skew +/-" ;
            rdf:value 6 ;
        ] ;
        lv2:portProperty lv2:integer ;
    ] ,
    [
        a lv2:InputPort, lv2:ControlPort ;
        lv2:index 15 ;
        lv2:name "V Warp Amount" ;
        lv2:symbol "vwarpamount" ;
        lv2:default 0 ;
        lv2:minimum 0 ;
        lv2:maximum 1 ;
    ] ,
    [
        a lv2:OutputPort, lv2:ControlPort ;
        lv2:index 16 ;
        lv2:name "Out" ;
        lv2:symbol "out" ;
        lv2:minimum 0 ;
        lv2:maximum 1 ;
    ] ;

    rdfs:comment "Waveshaping distortion plugin with spline-based graph." ;

    mod:brand "Wolf Plugins" ;
    mod:label "Wolf Shaper" ;

    doap:name "Wolf Shaper" ;
    doap:license <http://spdx.org/licenses/GPL-3.0-or-later.html> ;

    doap:maintainer [
        foaf:name "Patrick Desaulniers" ;
        foaf:homepage <https://github.com/pdesaulniers/wolf-shaper> ;
    ] ;

    lv2:microVersion 0 ;
    lv2:minorVersion 2 .
