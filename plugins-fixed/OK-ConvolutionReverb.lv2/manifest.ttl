@prefix lv2:  <http://lv2plug.in/ns/lv2core#> .
@prefix mod:  <http://moddevices.com/ns/mod#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix pset: <http://lv2plug.in/ns/ext/presets#> .
@prefix state: <http://lv2plug.in/ns/ext/state#> .

<https://mod.audio/plugins/ConvolutionReverb>
    a lv2:Plugin ;
    lv2:binary <OK-ConvolutionReverb_dsp.so> ;
    rdfs:seeAlso <OK-ConvolutionReverb_dsp.ttl> , <modgui.ttl> .

<https://mod.audio/plugins/ConvolutionReverb#irfile>
    a lv2:Parameter ;
    rdfs:label "IR File" ;
    mod:fileTypes "ir" ; 
    rdfs:range <http://lv2plug.in/ns/ext/atom#Path> .

<https://mod.audio/plugins/ConvolutionReverb#preset001>
    a pset:Preset ;
    lv2:appliesTo <https://mod.audio/plugins/ConvolutionReverb> ;
    rdfs:label "Default (medium hall)";
    lv2:port [
        lv2:symbol "wetlevel" ;
        pset:value -30 ;
    ] ,
    [
        lv2:symbol "drylevel" ;
        pset:value 0 ;
    ] ,
    [
        lv2:symbol "hpf" ;
        pset:value 0 ;
    ] ,
    [
        lv2:symbol "trails" ;
        pset:value 1 ;
    ] ,
    [
        lv2:symbol "lv2_enabled" ;
        pset:value 1 ;
    ] ;
    state:state [
        <https://mod.audio/plugins/ConvolutionReverb#irfile> <hall1-medium.flac>;
    ] .
