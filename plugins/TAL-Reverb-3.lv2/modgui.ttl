@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .

<urn:juce:TalReverb3>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-tal-reverb-iii.html> ;
        modgui:stylesheet <modgui/stylesheet-tal-reverb-iii.css> ;
        modgui:screenshot <modgui/screenshot-tal-reverb-iii.png> ;
        modgui:thumbnail <modgui/thumbnail-tal-reverb-iii.png> ;
        modgui:brand "TAL" ;
        modgui:label "Reverb-III" ;
        modgui:model "boxy" ;
        modgui:panel "7-knobs" ;
        modgui:color "racing" ;
        modgui:knob "black" ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "pre_delay" ;
            lv2:name "pre delay" ;
        ] , [
            lv2:index 1 ;
            lv2:symbol "room_size" ;
            lv2:name "room size" ;
        ] , [
            lv2:index 2 ;
            lv2:symbol "low_shelf_gain" ;
            lv2:name "low shelf gain" ;
        ] , [
            lv2:index 3 ;
            lv2:symbol "high_shelf_gain" ;
            lv2:name "high shelf gain" ;
        ] , [
            lv2:index 4 ;
            lv2:symbol "stereo" ;
            lv2:name "stereo" ;
        ] , [
            lv2:index 5 ;
            lv2:symbol "dry" ;
            lv2:name "dry" ;
        ] , [
            lv2:index 6 ;
            lv2:symbol "wet" ;
            lv2:name "wet" ;
        ] ;
    ] .
